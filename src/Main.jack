// Main.jack
class Main {
    function void main() {
        // Объявление переменной типа TetrisGame
        var TetrisGame game;

        // Создание нового объекта игры
        let game = TetrisGame.new();

        // Запуск игрового процесса
        do game.run();

        // Освобождение ресурсов после завершения игры
        do game.dispose();

        return;
    }
}
